<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane fx:id="rootgridpane" style="-fx-background-color: #D9F7F0;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.intershala.JAVAFX.Controller">
    <columnConstraints>
        <ColumnConstraints />
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="74.0" minHeight="10.0" prefHeight="71.0" />
        <RowConstraints maxHeight="687.0" minHeight="648.0" prefHeight="651.0" />
    </rowConstraints>
    <children>
        <Pane GridPane.columnSpan="2" />
      <Pane fx:id="insertedDiscpane" prefHeight="666.0" prefWidth="200.0" GridPane.rowIndex="1" />
      <VBox style="-fx-background-color: #2B3B4C;" GridPane.columnIndex="1" GridPane.rowIndex="1">
         <children>
            <TextField fx:id="playeronetextfeild" prefHeight="48.0" prefWidth="269.0" promptText="Player One Name">
               <font>
                  <Font size="18.0" />
               </font>
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="playertwotextfeild" prefHeight="48.0" prefWidth="269.0" promptText="Player Two Name">
               <font>
                  <Font size="18.0" />
               </font>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </TextField>
            <Button fx:id="setbutton" mnemonicParsing="false" text="Set Names">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
               <VBox.margin>
                  <Insets left="80.0" top="20.0" />
               </VBox.margin></Button>
            <Label fx:id="playerIdlabel" alignment="CENTER" prefHeight="168.0" prefWidth="271.0" text="Player One" textFill="#f0f0f0">
               <font>
                  <Font name="Arial Black" size="34.0" />
               </font>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Label>
            <Label alignment="CENTER" prefHeight="39.0" prefWidth="264.0" text="Turn" textFill="#fffafa">
               <font>
                  <Font name="System Bold" size="27.0" />
               </font>
            </Label>
            <Region prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
            <Region prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
    </children>
</GridPane>
